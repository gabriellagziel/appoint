{
  "displayName": "Free Access Backlog Alert",
  "documentation": {
    "content": "Alert when there are more than 1000 active grants that have expired but haven't been processed",
    "mimeType": "text/markdown"
  },
  "conditions": [
    {
      "displayName": "Free Access Backlog High",
      "conditionThreshold": {
        "filter": "metric.type=\"custom.googleapis.com/free_access_backlog\"",
        "comparison": "COMPARISON_GREATER_THAN",
        "thresholdValue": 1000,
        "duration": "300s",
        "aggregations": [
          {
            "alignmentPeriod": "60s",
            "perSeriesAligner": "ALIGN_MEAN",
            "crossSeriesReducer": "REDUCE_MEAN"
          }
        ]
      }
    }
  ],
  "alertStrategy": {
    "autoClose": "3600s"
  },
  "combiner": "OR",
  "enabled": true,
  "notificationChannels": [
    "projects/app-oint/notificationChannels/123456789"
  ]
}
