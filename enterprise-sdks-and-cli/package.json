{
    "name": "appoint-enterprise-sdks",
    "version": "1.0.0",
    "description": "Official client libraries and CLI for App-Oint Enterprise",
    "main": "index.js",
    "scripts": {
        "build": "npm run build:js && npm run build:python && npm run build:java && npm run build:dotnet",
        "build:js": "cd javascript && npm run build",
        "build:python": "cd python && python setup.py build",
        "build:java": "cd java && mvn clean package",
        "build:dotnet": "cd dotnet && dotnet build",
        "publish": "npm run publish:js && npm run publish:python && npm run publish:java && npm run publish:dotnet",
        "publish:js": "cd javascript && npm publish",
        "publish:python": "cd python && python setup.py sdist upload",
        "publish:java": "cd java && mvn deploy",
        "publish:dotnet": "cd dotnet && dotnet pack && dotnet nuget push",
        "test": "npm run test:js && npm run test:python && npm run test:java && npm run test:dotnet",
        "test:js": "cd javascript && npm test",
        "test:python": "cd python && python -m pytest",
        "test:java": "cd java && mvn test",
        "test:dotnet": "cd dotnet && dotnet test",
        "cli:build": "cd cli && npm run build",
        "cli:publish": "cd cli && npm publish"
    },
    "keywords": [
        "appoint",
        "enterprise",
        "sdk",
        "api",
        "client",
        "javascript",
        "python",
        "java",
        "dotnet",
        "cli"
    ],
    "author": "App-Oint Team",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^18.0.0",
        "eslint": "^8.0.0",
        "jest": "^29.0.0",
        "prettier": "^2.8.0",
        "typescript": "^4.9.0"
    },
    "workspaces": [
        "javascript",
        "python",
        "java",
        "dotnet",
        "cli"
    ],
    "dependencies": {
        "@octokit/rest": "^22.0.0",
        "firebase-admin": "^13.4.0"
    }
}
