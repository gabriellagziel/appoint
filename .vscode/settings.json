{
	"files.exclude": {
		"**/node_modules/**": true,
		"**/.npm-cache/**": true,
		"**/build/**": true,
		"**/cleanup_backup_*/**": true,
		"**/todo_fixme_backup_*/**": true,
		"**/translation_backup/**": true
	},
	"search.exclude": {
		"**/node_modules/**": true,
		"**/.npm-cache/**": true,
		"**/build/**": true,
		"**/cleanup_backup_*/**": true,
		"**/todo_fixme_backup_*/**": true,
		"**/translation_backup/**": true
	},
	"dart.analysisExcludedFolders": [
		"${workspaceFolder}/admin",
		"${workspaceFolder}/business",
		"${workspaceFolder}/dashboard",
		"${workspaceFolder}/enterprise-app",
		"${workspaceFolder}/enterprise-onboarding-portal",
		"${workspaceFolder}/enterprise-sdks-and-cli",
		"${workspaceFolder}/functions",
		"${workspaceFolder}/marketing",
		"${workspaceFolder}/packages",
		"${workspaceFolder}/scripts",
		"${workspaceFolder}/tools",
		"${workspaceFolder}/web",
		"${workspaceFolder}/cleanup_backup_*",
		"${workspaceFolder}/todo_fixme_backup_*",
		"${workspaceFolder}/translation_backup"
	],
	"eslint.enable": false,
	"typescript.validate.enable": false,
	"javascript.validate.enable": false,
	"typescript.tsserver.enable": false,
	"typescript.tsserver.experimental.enableProjectDiagnostics": false,
  "typescript.disableAutomaticTypeAcquisition": true,
  "css.lint.unknownAtRules": "ignore",
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false
}