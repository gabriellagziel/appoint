git_url("https://github.com/gabriellagziel/appoint-certificates")
storage_mode("git")

type("development") # The default type, can be: appstore, adhoc, enterprise or development

app_identifier(["com.example.appoint"]) # The bundle identifier of your app
username("your-apple-id@example.com") # Your Apple Developer Portal username

# For all available options run `fastlane match --help`
# The docs are available on https://docs.fastlane.tools/actions/match/

