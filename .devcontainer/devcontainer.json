{
  "name": "APP-OINT Dev Container",
  "image": "mcr.microsoft.com/vscode/devcontainers/flutter:stable",
  "features": {
    "ghcr.io/devcontainers/features/firebase-cli:1": {}
  },
  "extensions": [
    "Dart-Code.dart-code",
    "Dart-Code.flutter"
  ],
  "postCreateCommand": "flutter pub get && dart pub global activate flutterfire_cli",
  "remoteUser": "vscode"
}
