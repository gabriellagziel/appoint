    "dashboards": {
      "api_key_dashboard": {
        "enabled": true,
        "refresh_interval": "30 seconds"
      },
      "enterprise_dashboard": {
        "enabled": true,
        "refresh_interval": "5 minutes"
      }
    },
    "dashboard_link": "https://monitoring.appoint.com/enterprise",
    "blockers": null
  },
