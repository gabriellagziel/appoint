{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "lib": ["esnext"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "REDACTED_TOKEN": true,
    "outDir": "build"
  },
  "include": [
    "functions/src/**/*",
    "pages/**/*"
  ],
  "exclude": [
    "node_modules",
    ".next",
    "build",
    "dist"
  ]
}
